    Private Sub btnOK_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnOK.Click
        If TextBox1.Text.Length = 0 Or TextBox2.Text.Length = 0 Then
            MessageBox.Show("Please input all parameters and then select the directory path to save in!", "Warning!")
            Me.TextBox1.Focus()
        Else
            Me.Cursor = Cursors.WaitCursor
            Me.PictureBox1.Visible = True
            Me.PictureBox1.Update()
            Me.btnOK.Enabled = False
            Me.Label4.Visible = False
            Me.Label4.Update()
            Me.Label3.Text = "Processing... Please Wait!"
            Me.Label3.Update()
            

            Dim s1 As FileStream 'Load file 1
            Dim s3 As FileStream 'Save output

            If System.IO.File.Exists(TextBox2.Text) Then
                System.IO.File.Delete(TextBox2.Text)
            End If

            '---read from and write to a binary file
            s1 = New FileStream(TextBox1.Text, FileMode.Open, FileAccess.Read)
            s3 = New FileStream(TextBox2.Text, FileMode.CreateNew, FileAccess.Write)

            Dim br1 As BinaryReader
            Dim bw As BinaryWriter

            br1 = New BinaryReader(s1)
            bw = New BinaryWriter(s3)

            Dim fLen1 As Integer
            Dim f1 As New System.IO.FileInfo(TextBox1.Text)
            fLen1 = f1.Length

            'Dim snglRead1(fLen1) As Single
            'Dim snglOutput(fLen1) As Single 'wıll hold results from division
            Dim intRead As Integer
            Dim snglRead As Single
            Dim snglOutput As Single 'wıll hold results from division

            Dim i As Integer
            'Dim m As Integer

            Dim class_0 As Single
            Dim class_1 As Single
            Dim class_2 As Single
            Dim class_3 As Single
            Dim class_4 As Single
            Dim class_5 As Single
            Dim class_6 As Single
            Dim class_7 As Single
            Dim class_8 As Single
            Dim class_9 As Single
            Dim class_10 As Single
            Dim class_11 As Single

            'Read the values in textboxes and convert them to singles
            class_0 = CSng(TextBox3.Text)
            class_1 = CSng(TextBox4.Text)
            class_2 = CSng(TextBox5.Text)
            class_3 = CSng(TextBox6.Text)
            class_4 = CSng(TextBox7.Text)
            class_5 = CSng(TextBox8.Text)
            class_6 = CSng(TextBox9.Text)
            class_7 = CSng(TextBox10.Text)
            class_8 = CSng(TextBox11.Text)
            class_9 = CSng(TextBox12.Text)
            class_10 = CSng(TextBox13.Text)
            class_11 = CSng(TextBox14.Text)

           
            'Read the classified binary file  
            For i = 0 To fLen1 - 1
                intRead = br1.ReadByte

                Select Case intRead
                    Case 0
                        snglOutput = class_0
                    Case 1
                        snglOutput = class_1
                    Case 2
                        snglOutput = class_2
                    Case 3
                        snglOutput = class_3
                    Case 4
                        snglOutput = class_4
                    Case 5
                        snglOutput = class_5
                    Case 6
                        snglOutput = class_6
                    Case 7
                        snglOutput = class_7
                    Case 8
                        snglOutput = class_8
                    Case 9
                        snglOutput = class_9
                    Case 10
                        snglOutput = class_10
                    Case 11
                        snglOutput = class_11

                End Select

                bw.Write(snglOutput)
            Next

            s1.Close()
            s3.Close()

            Me.PictureBox1.Visible = False
            Me.PictureBox2.Visible = True
            'Me.Label3.Visible = False
            'Me.Label3.Update()
            'Me.Label4.Text = "Process Completed!"
            'Me.Label4.Update()

            Me.Label3.Visible = False
            Me.Label3.Update()
            Me.Label4.Visible = True
            Me.Label4.Text = "Process Completed!"
            Me.Label4.Update()

            btnCancel.Text = "Close"
            Me.Cursor = Cursors.Default
            MessageBox.Show("File created successfully!", "Success!")
            'Me.Close()
        End If

    End Sub

